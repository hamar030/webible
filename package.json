{
  "name": "webible",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxi dev --dotenv .env.local.dev",
    "dev:ntl": "ntl dev",
    "dev:prepare": "nuxi build-module build --stub && nuxi build-module --prepare && nuxi prepare",
    "dev:preview:build": "dev:build && node .output/server/index.mjs",
    "build": "nuxi build",
    "build:bun": "SERVER_PRESET=bun nuxi build",
    "build:ntl": "ntl build",
    "generate": "nuxi generate",
    "generate:ntl": "SERVER_PRESET=netlify-edge nuxi generate",
    "preview": "nuxi preview --dotenv .env.local.production",
    "preview:ntl": "ntl serve",
    "prepack": "nuxi build-module --prepare && nuxi build-module build",
    "postinstall": "nuxi prepare",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fontsource-variable/heebo": "^5.0.21",
    "@fontsource-variable/nunito": "^5.0.18",
    "@fontsource-variable/nunito-sans": "^5.0.13",
    "@pinia/nuxt": "^0.5.1",
    "@trpc/client": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "@types/lodash": "^4.14.202",
    "@vite-pwa/nuxt": "^0.5.0",
    "@vueuse/nuxt": "^10.8.0",
    "lodash": "^4.17.21",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "trpc-nuxt": "^0.10.20",
    "vue": "^3.4.19",
    "vue-lodash": "^2.1.2",
    "vue-router": "^4.2.5",
    "yaml": "^2.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.1.1",
    "@netlify/blobs": "^7.0.1",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "autoprefixer": "^10.4.18",
    "cssnano": "^6.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "nuxt": "^3.11.1",
    "nuxt-headlessui": "^1.1.5",
    "nuxt-lodash": "^2.5.3",
    "postcss": "^8.4.35",
    "postcss-apply": "^0.12.0",
    "postcss-import": "^16.0.1",
    "postcss-nesting": "^12.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.1",
    "vite": "^5.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^1.8.27"
  }
}
