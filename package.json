    "@vueuse/nuxt": "^10.8.0",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "trpc-nuxt": "^0.10.19",
    "vue": "^3.4.19",
    "vue-lodash": "^2.1.2",
    "vue-router": "^4.2.5",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.1.1",
