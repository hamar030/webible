{
  "name": "webible",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepack": "nuxi build-module --prepare && nuxi build-module build",
    "dev": "nuxi dev --dotenv .env.local",
    "dev:netlify": "ntl dev",
    "dev:generate:netlify": "SERVER_PRESET=netlify-edge nuxi generate --dotenv .env.local",
    "dev:build": "nuxi build --dotenv .env.local",
    "dev:build:netlify": "SERVER_PRESET=netlify-edge nuxi build --dotenv .env.local",
    "dev:prepare": "nuxi build-module build --stub && nuxi build-module --prepare && nuxi prepare",
    "dev:preview:build": "nr dev:build && node .output/server/index.mjs",
    "build": "ntl build --dotenv .env.production",
    "generate": "nuxi generate --dotenv .env.production",
    "preview": "ntl serve --dotenv .env.production",
    "postinstall": "nuxi prepare",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fontsource-variable/heebo": "^5.0.17",
